{
    "python.pythonPath": "venv/bin/python",
    "files.associations": {
        "*.html": "jinja-html"
    },
    "emmet.includeLanguages": {
        "jinja-html": "html"
    },
    "sqltools.connections": [
        {
            "previewLimit": 50,
            "driver": "SQLite",
            "database": "./app.db",
            "name": "microblogSQLiteDB"
        }
    ],
    "sqltools.useNodeRuntime": true
}